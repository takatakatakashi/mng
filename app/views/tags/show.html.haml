.category
  -@mangas.each do |manga|
    %img{height: "600", width: "450", src: manga.image}
    .category_1
      = link_to '詳細', "/mangas/#{manga.id}", method: :get
        - if user_signed_in? && current_user.id == manga.user_id
      .category_2
        =manga.name
        .category_3
          =manga.text
       